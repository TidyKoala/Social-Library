
<form action="{{ path('object_creator_ajax_create') }}" method="post" {{ form_enctype(form) }} class="ajax-submit">
    {{ form_widget(form.firstname, { 'attr' : { 'placeholder' : 'placeholder_firstname_creator' } }) }}
    {{ form_errors(form.firstname) }}
    {{ form_widget(form.lastname, { 'attr' : { 'placeholder' : 'placeholder_lastname_creator' } }) }}
    {{ form_errors(form.lastname) }}
    {{ form_rest(form) }}
    <button type="submit" class="btn btn-primary">
        {% trans %}create_add{% endtrans %}
    </button>
</form>
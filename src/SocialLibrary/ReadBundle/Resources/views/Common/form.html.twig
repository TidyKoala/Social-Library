                        {{ form_widget(form.pictureFile) }}
                    </span>
                    <a href="#" class="btn fileupload-exists" data-dismiss="fileupload">{% trans %}label_picture_remove{% endtrans %}</a>
                </div>
            </div>
            <span class="help-inline">
                {{ form_errors(form.pictureFile) }}
            </span>
        </div>
